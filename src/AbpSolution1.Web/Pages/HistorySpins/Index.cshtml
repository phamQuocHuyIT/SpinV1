@page
@inject IStringLocalizer<AbpSolution1Resource> L
@inject IAuthorizationService AuthorizationService
@using AbpSolution1.Localization
@using AbpSolution1.Permissions
@using Microsoft.AspNetCore.Authorization
@using Microsoft.Extensions.Localization
@model AbpSolution1.Web.Pages.HistorySpins.IndexModel
@{
}
@section scripts
{
    <abp-script src="/Pages/HistorySpins/Index.js" />
}
<abp-card>
    <abp-card-header>
        <abp-row>
            <abp-column size-md="_6">
                <abp-card-title>@L["HistorySpin"]</abp-card-title>
            </abp-column>
            
            <abp-row class="mt-3">
                <abp-column size-md="_6">
                    <input type="text" id="HistorySpinsearch" class="form-control" placeholder="@L["SearchHistorySpin"]" />
                </abp-column>
            </abp-row>
        </abp-row>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="HistorySpinTable"></abp-table>
    </abp-card-body>
</abp-card>